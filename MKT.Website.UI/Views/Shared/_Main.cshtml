<!DOCTYPE html>

<html >
<head>

    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="description" content=@_localization.Getkey("str-meta-description")>
    <meta name="author" content="Tech Nexus" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="robots" content="max-snippet:20, max-image-preview:large">

    <link href="~/css/particles/style.css" rel="stylesheet" media="screen" />

    


	

</head>
<body>


    <div id="particles-js" >

        
            <div id="MainSentances" class="main-sentances" style="text-align:center; font-size: 2.5vw;" dir=@_localization.Getkey("lang_dir")>
                @_localization.Getkey("str_main_message_1")

            <br />
                <span class="auto-type"> </span>
                <br />
            </div>

        </div>
        

    
    <div>
        @RenderBody()
    </div>

    <script src="~/js/particles/lib/particles.min.js"></script>
    <script src="~/js/particles/app.js"></script>




</body>
</html>


